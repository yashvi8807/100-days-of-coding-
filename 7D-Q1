// Q13: Write a program to input a year and check whether it is a leap year or not using conditional statements.
#include<stdio.h>
int main()
{
    int y;
    printf("Enter year");
    scanf("%d",&y);
    if(y % 400 == 0)
    {
        printf("leap year");
    }
    else if(y % 100 == 0)
    {
        printf("Not aleap year");
    }
    else if(y % 4 == 0){
        printf("leap year");
    }
    else {
        printf("Not a leap year");
    }

    return 0;
    
}
